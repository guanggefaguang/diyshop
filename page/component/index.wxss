@import '../common/common.wxss';

page {
  background: #f5f5f5;
}

.swiper-box {
  width: 100%;
  height: 400rpx;
}

swiper-item {
  width: 100%;
  height: 400rpx;
}

.Img {
  width: 100%;
  height: 400rpx;
}

.shop {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}

.Home_title {
  margin: 20rpx 0rpx;
  text-align: center;
  color: #ab956d;
  font-size: 36rpx;
  font-weight: bold;
  letter-spacing: 4rpx;
  position: relative;
}

.Home_title text {
  background: #f5f5f5;
  padding: 0rpx 10rpx;
}

.xie {
  position: absolute;
  z-index: -1;
  width: 400rpx;
  height: 1rpx;
  top: 50%;
  left: 180rpx;
  background: #ab956d;
}
